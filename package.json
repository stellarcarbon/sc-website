{
  "name": "sc-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "demo": "NEXT_PUBLIC_DEMO_VERSION=true next dev",
    "build": "next build",
    "start": "next start",
    "start-static": "npx serve@latest out",
    "lint": "next lint",
    "e2e": "cypress open --e2e",
    "e2e:headless": "cross-env NODE_ENV=test next build && cross-env NODE_ENV=test start-server-and-test start http://localhost:3000 \"cypress run --e2e\"",
    "test": "vitest",
    "test:ci": "vitest run",
    "generate-client": "openapi --useOptions --input https://api-beta.stellarcarbon.io/openapi.json --output ./src/client --client axios",
    "generate-client-local": "openapi --useOptions --input http://localhost:8000/openapi.json --output ./src/client --client axios"
  },
  "dependencies": {
    "@creit.tech/stellar-wallets-kit": "^1.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/error-message": "^2.0.1",
    "@stellar/stellar-sdk": "^12.2.0",
    "google-fonts-plugin": "^5.0.2",
    "i": "^0.3.7",
    "next": "14.0.4",
    "npm": "^10.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.2",
    "react-loader-spinner": "^6.1.6",
    "react-swipeable": "^7.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "fake-indexeddb": "^5.0.2",
    "form-data": "^4.0.0",
    "openapi-typescript-codegen": "^0.27.0",
    "pino-pretty": "^10.3.1",
    "postcss": "^8",
    "serve": "^14.2.2",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^1.2.2"
  }
}
